ALTER TABLE CUSTOMER  
MODIFY (creATED DEFAULT sysdate );

select 'ALTER TABLE  '||table_name||' MODIFY (UPDATED DEFAULT sysdate );' from user_tables a
union all
select 'ALTER TABLE  '||table_name||' MODIFY (creATED DEFAULT sysdate );' from user_tables a


